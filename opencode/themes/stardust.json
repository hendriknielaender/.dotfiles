    "diffLineNumber": {
      "dark": "stardust_context",
      "light": "stardust_context"
    },
    "diffAddedLineNumberBg": {
      "dark": "#2e3330",
      "light": "#acd2ac"
    },
    "diffRemovedLineNumberBg": {
      "dark": "#2e3330",
      "light": "#D16969"
    },
