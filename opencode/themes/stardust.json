    "borderSubtle": {
      "dark": "stardust_context",
      "light": "stardust_light_gray"
    },
    "diffAdded": {
      "dark": "stardust_green",
      "light": "stardust_green"
    },
    "diffRemoved": {
      "dark": "stardust_red",
      "light": "stardust_red"
    },
    "diffContext": {
      "dark": "stardust_context",
      "light": "stardust_context"
    },
    "diffHunkHeader": {
      "dark": "stardust_light_gray",
      "light": "stardust_light_gray"
    },
    "diffHighlightAdded": {
      "dark": "stardust_light_green",
      "light": "stardust_light_green"
    },
    "diffHighlightRemoved": {
      "dark": "stardust_light_red",
      "light": "stardust_light_red"
