      "dark": "stardust_fg",
      "light": "stardust_dark"
    },
    "markdownHeading": {
      "dark": "stardust_blue",
      "light": "stardust_blue"
    },
    "markdownLink": {
      "dark": "stardust_cyan",
      "light": "stardust_cyan"
    },
    "markdownLinkText": {
      "dark": "stardust_light_blue",
      "light": "stardust_light_blue"
    },
    "markdownCode": {
      "dark": "stardust_string",
      "light": "stardust_string"
    },
    "markdownBlockQuote": {
      "dark": "stardust_light_gray",
      "light": "stardust_context"
    },
    "markdownEmph": {
      "dark": "stardust_yellow",
      "light": "stardust_yellow"
    },
    "markdownStrong": {
      "dark": "stardust_accent",
      "light": "stardust_accent"
    },
    "markdownHorizontalRule": {
      "dark": "stardust_context",
      "light": "stardust_context"
    },
    "markdownListItem": {
      "dark": "stardust_cyan",
      "light": "stardust_cyan"
    },
    "markdownListEnumeration": {
      "dark": "stardust_purple",
      "light": "stardust_purple"
    },
    "markdownImage": {
      "dark": "stardust_magenta",
      "light": "stardust_magenta"
    },
    "markdownImageText": {
      "dark": "stardust_light_blue",
      "light": "stardust_light_blue"
    },
    "markdownCodeBlock": {
      "dark": "stardust_fg",
      "light": "stardust_dark"
    },
    "syntaxComment": {
      "dark": "stardust_green",
      "light": "stardust_green"
    },
    "syntaxKeyword": {
      "dark": "stardust_blue",
      "light": "stardust_blue"
    },
    "syntaxFunction": {
      "dark": "stardust_func",
      "light": "stardust_func"
    },
    "syntaxVariable": {
      "dark": "stardust_fg",
      "light": "stardust_dark"
    },
    "syntaxString": {
      "dark": "stardust_string",
      "light": "stardust_string"
    },
    "syntaxNumber": {
      "dark": "stardust_number",
      "light": "stardust_number"
    },
    "syntaxType": {
      "dark": "stardust_accent",
      "light": "stardust_accent"
    },
    "syntaxOperator": {
      "dark": "stardust_cyan",
      "light": "stardust_cyan"
    },
    "syntaxPunctuation": {
      "dark": "stardust_light_gray",
      "light": "stardust_context"
    }
  }
}
